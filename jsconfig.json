      "@/api/*": ["lib/api/*"],
      "@/queries/*": ["queries/*"],
      "@/components/*": ["components/*"],
      "@/blocks/*": ["components/blocks/*"],
      "@/dynamic/*": ["components/dynamic/*"],
      "@/factories/*": ["components/factories/*"],
      "@/atomics/*": ["components/atomics/*"],
